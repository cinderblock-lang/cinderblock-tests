namespace test.io {
  unsafe fn sys_print(data: string, length: int): int {
    return system(1i, 1i, data, length);
  }

  export unsafe fn log(data: string): int {
    store input = data.concat("\n");
    return sys_print(input, input.length());
  }
}

namespace App {
  using test.io;

  fn FunctionTest(): string {
    store message = "This is from a function";

    return message;
  }

  export unsafe fn main(): int {
    store message = FunctionTest();

    return message.log();
  }
}