namespace test.io {
  export fn log(data: string): int {
    store input = data.concat("\n");
    return sys_print(input, input.length());
  }
}

namespace App {
  using test.io;

  fn FunctionTest(): string {
    store message = "This is from a function";

    return message;
  }

  export fn main(): int {
    store message = FunctionTest();

    return message.log();
  }
}