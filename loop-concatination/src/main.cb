namespace App {
  using Console;

  export unsafe fn main(): int {
    iterator1 -> iterate [increment(2i)] (index) -> "Performing a loop ";
    iterator2 -> iterate [increment(2i)] (index) -> "Performing a loop ";

    message -> reduce[iterator1 ++ iterator2, ""] (item, current) -> current.concat(item);

    return message.Log();
  }
}